{
    "name": "robotframework",
    "displayName": "robot language",
    "description": "robot language support for vscode",
    "version": "0.1.4",
    "publisher": "keith",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:robot"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "languages": [
            {
                "id": "robot",
                "aliases": [
                    "robotframework"
                ],
                "extensions": [
                    ".txt",
                    ".rst"
                ]
            }
        ],
        "grammars": [
            {
                "language": "robot",
                "scopeName": "text.robot",
                "path": "./syntaxes/robot.tmLanguage"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "icon": "Capture.PNG",
    "repository": "https://github.com/WindSoilder/robot-language-for-vscode"
}